        <!--android:background="#ffffffff">-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home$PlaceholderFragment"
    android:orientation="vertical"
    android:background="#ffffffff"
    android:id="@+id/fragment">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/feed"
        android:contentDescription="@string/feed"
        android:background="@drawable/rss"
        android:clickable="true"
        android:onClick="onClickF"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/uv" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/calculadora"
        android:contentDescription="@string/calculadora"
        android:background="@drawable/cal"
        android:clickable="true"
        android:onClick="onClickC"
        android:layout_below="@+id/feed"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="82dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/uv"
        android:contentDescription="uv"
        android:clickable="true"
        android:onClick="onClickUV"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/uv" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/maps"
        android:layout_alignTop="@+id/calculadora"
        android:layout_alignRight="@+id/uv"
        android:layout_alignEnd="@+id/uv"
        android:contentDescription="map"
        android:onClick="onClickMaps"
        android:clickable="true"
        android:background="@drawable/map" />

</RelativeLayout>
    <!--</LinearLayout>-->
</ScrollView>